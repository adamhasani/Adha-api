{
  "name": "Ada API UI",
  "version": "v1.0.0",
  "description": "Simple and easy to use API console for Ada API.",
  "bannerImage": "/src/banner.gif",

  "apiBaseUrl": "https://ada-api-vace.vercel.app",
  "copyBaseUrl": "https://ada-api-vace.vercel.app",

  "header": {
    "status": "Online!"
  },

  "apiSettings": {
    "creator": "Ada",
    "apikey": ["ada-api"]
  },

  "categories": [
    {
      "name": "AI (Artificial Intelligence)",
      "items": [
        {
          "name": "LuminAI",
          "desc": "Chat dengan LuminAI. Contoh: text=halo dari web",
          "path": "/ai/luminai?text=halo",
          "status": "ready",
          "method": "GET",
          "params": {
            "text": "Text input"
          }
        }
      ]
    },

    {
      "name": "Downloader",
      "items": [
        {
          "name": "YouTube MP3",
          "desc": "Download audio dari YouTube (klik START untuk menjalankan).",
          "path": "/api/download/ytmp3?url=https%3A%2F%2Fyoutu.be%2FVIDEO_ID",
          "status": "ready",
          "method": "GET",
          "params": {
            "url": "YouTube video URL"
          }
        }
      ]
    },

    {
      "name": "Search Tools",
      "items": [
        {
          "name": "YouTube Search",
          "desc": "Cari video YouTube (klik START untuk menjalankan).",
          "path": "/search/youtube?q=lofi%20hip%20hop",
          "status": "ready",
          "method": "GET",
          "params": {
            "q": "Search query"
          }
        }
      ]
    },

    {
      "name": "Random",
      "items": [
        {
          "name": "Blue Archive",
          "desc": "Random Blue Archive image.",
          "path": "/random/ba",
          "status": "ready",
          "method": "GET"
        }
      ]
    },

    {
      "name": "Tools",
      "items": [
        {
          "name": "Tourl (Upload)",
          "desc": "Upload file menjadi URL.",
          "path": "/api/tools/tourl",
          "status": "ready",
          "method": "POST",
          "params": {
            "file": "File upload"
          }
        }
      ]
    }
  ]
}
